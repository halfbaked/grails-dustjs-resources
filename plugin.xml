<plugin name='dustjs' version='0.1' grailsVersion='2.0.0 &gt; *'>
  <author>Zach Legein &amp; Zach Lendon</author>
  <title>Dustjs  Plugin</title>
  <description>\
This plugin supports server-side compilation of .dust template files to their .js counterparts.
</description>
  <documentation>http://grails.org/plugin/dustjs-plugin</documentation>
  <type>DustjsGrailsPlugin</type>
  <resources>
    <resource>DustjsResourceMapper</resource>
    <resource>DustjsCompilerException</resource>
    <resource>DustjsEngineService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://snapshots.repository.codehaus.org' url='http://snapshots.repository.codehaus.org/' />
    <repository name='http://repository.codehaus.org' url='http://repository.codehaus.org/' />
    <repository name='http://m2repo.spockframework.org/snapshots' url='http://m2repo.spockframework.org/snapshots/' />
  </repositories>
  <dependencies />
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='jquery' version='1.7' />
      <plugin group='org.grails.plugins' name='resources' version='1.1.3' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.0.0' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='greenmail' version='1.2.1' />
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2' />
      <plugin group='org.grails.plugins' name='database-migration' version='1.0' />
      <plugin group='org.grails.plugins' name='build-test-data' version='1.1.0' />
      <plugin group='org.grails.plugins' name='json-rest-api' version='1.0.11' />
      <plugin group='org.grails.plugins' name='mail' version='0.9' />
      <plugin group='org.grails.plugins' name='spock' version='0.6-SNAPSHOT' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.0.0' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>