<plugin name='dustjs' version='0.2' grailsVersion='2.0 &gt; *'>
  <author>Zach Legein &amp; Zach Lendon</author>
  <title>Dustjs  Plugin</title>
  <description>\
This plugin supports server-side compilation of .dust template files to their .js counterparts.
</description>
  <documentation>http://grails.org/plugin/dustjs-plugin</documentation>
  <type>DustjsGrailsPlugin</type>
  <resources>
    <resource>DustjsResourceMapper</resource>
    <resource>DustjsCompilerException</resource>
    <resource>DustjsEngineService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
    <repository name='http://m2repo.spockframework.org/snapshots' url='http://m2repo.spockframework.org/snapshots/' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='org.seleniumhq.selenium' name='selenium-firefox-driver' version='2.5.0' />
      <dependency group='org.codehaus.geb' name='geb-spock' version='0.6.2' />
    </test>
  </dependencies>
  <plugins />
  <runtimePluginRequirements>
    <plugin name='resources' version='1.1.3 &gt; *' />
  </runtimePluginRequirements>
  <behavior />
</plugin>