<plugin name='dustjs-resources' version='0.2' grailsVersion='1.3.7 &gt; *'>
  <author>Zach Legein</author>
  <authorEmail>zlegein@gmail.com</authorEmail>
  <title>DustJs  Resource Plugin</title>
  <description>This plugins supports server-side compilation of .dust template files to their .js counterparts.</description>
  <documentation>http://grails.org/plugin/grails-dustjs-resource</documentation>
  <type>DustjsResourcesGrailsPlugin</type>
  <resources>
    <resource>DustjsResourceMapper</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <runtime>
      <dependency group='org.mozilla' name='rhino' version='1.7R3' />
    </runtime>
  </dependencies>
  <plugins>
    <runtime>
      <plugin group='org.grails.plugins' name='resources' version='1.1.6' />
    </runtime>
  </plugins>
  <runtimePluginRequirements>
    <plugin name='resources' version='1.1.6 &gt; *' />
  </runtimePluginRequirements>
  <behavior />
</plugin>